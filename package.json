{
  "name": "@timurxyz/statefuld",
  "version": "0.9.1",
  "description": "statefuld (stateful dirt daemon, stateful-dee) is a lightweight Angular persistence service  to make values of data properties of components/classes survive Init/Destroy circles throughout a user session",
  "repository": "git@github.com:timurxyz/statefuld.git",
  "author": "timur x <x@secdev.eu>",
  "license": "MIT",
  "$schema": "../node_modules/ng-packagr/package.schema.json",
  "peerDependencies": {
    "@angular/common": "^5.1.0",
    "@angular/core": "^5.1.0"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/public_api.ts"
    },
    "dest": "./dist/statefuld"
  },
  "dependencies": {
    "@angular/common": "~9.1.6",
    "@angular/compiler": "~9.1.6",
    "@angular/core": "~9.1.6",
    "@angular/platform-browser": "~9.1.6",
    "@angular/platform-browser-dynamic": "~9.1.6",
    "@angular/router": "~9.1.6",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/cli": "^9.1.6",
    "@angular/compiler-cli": "^9.1.7",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^7.0.0",
    "ng-packagr": "^9.1.3",
    "ts-node": "~8.3.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "build:lib": "ng-packagr -p package.json"
  }
}
