{
  "name": "@timurxyz/statefuld",
  "version": "0.9.1",
  "description": "statefuld (stateful dirt daemon, stateful-dee) is a lightweight Angular persistence service  to make values of data properties of components/classes survive Init/Destroy circles throughout a user session",
  "repository": "git@github.com:timurxyz/statefuld.git",
  "author": "timur x <x@secdev.eu>",
  "license": "MIT",
  "$schema": "../node_modules/ng-packagr/package.schema.json",
  "peerDependencies": {
    "@angular/common": "^8.0.0 || ^9.0.0",
    "@angular/core": "^8.0.0 || ^9.0.0"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/public_api.ts"
    },
    "dest": "./dist/statefuld",
    "whitelistedNonPeerDependencies": [
      "."
    ]
  },
  "dependencies": {
    "@angular/common": "^8.0.0 || ^9.0.0",
    "@angular/core": "^8.0.0 || ^9.0.0",
    "@angular/platform-browser": "^8.0.0 || ^9.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0 || ^9.0.0",
    "@angular/router": "^8.0.0 || ^9.0.0",
    "rxjs": "^6.5.0",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.0"
  },
  "devDependencies": {
    "typescript": "^3.4.3",
    "ng-packagr": "^9.1.3",
    "@angular/cli": "^9.0.0",
    "@angular/compiler": "^9.0.0",
    "@angular/compiler-cli": "^9.0.0",
    "@angular/platform-browser": "^9.0.0",
    "@angular/platform-browser-dynamic": "^9.0.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^6.0.0",
    "ts-node": "^8.0.0",
    "zone.js": "^0.10.0"
  },
  "scripts": {
    "build:lib": "ng-packagr -p package.json"
  }
}
