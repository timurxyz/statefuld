{
  "name": "@timurxyz/statefuld",
  "version": "0.0.0",
  "description": "statefuld (stateful dirt daemon, stateful-dee) is a lightweight Angular persistence service  to make values of data properties of components/classes survive Init/Destroy circles throughout a user session",
  "repository": "git@github.com:timurxyz/statefuld.git",
  "author": "timur x <x@secdev.eu>",
  "license": "MIT",
  "dependencies": {
    "@angular/common": ">=8 || >=9",
    "@angular/core": ">=8 || >=9",
    "@angular/platform-browser": ">=8 || >=9",
    "@angular/platform-browser-dynamic": ">=8 || >=9",
    "@angular/router": ">=8 || >=9",
    "rxjs": ">=6.5.0",
    "tslib": ">=1.10.0",
    "zone.js": ">=0.9.0"
  },
  "devDependencies": {
    "@angular/cli": ">=9",
    "@angular/compiler": ">=9",
    "@angular/compiler-cli": ">=9",
    "@angular/platform-browser": ">=9",
    "@angular/platform-browser-dynamic": ">=9",
    "@types/node": ">=12.11.0",
    "@typescript-eslint/eslint-plugin": ">2.30.0",
    "@typescript-eslint/parser": ">2.30.0",
    "eslint": ">6.0.0",
    "ng-packagr": ">=9",
    "ts-node": ">=8 || >=9",
    "typescript": "<3.9.0",
    "zone.js": ">=0.10.0"
  },
  "scripts": {
    "build:github": "env NPM_CONFIG_REGISTRY='https://npm.pkg.github.com/@timurxyz' ng-packagr -p ./src.statefuld/package.json",
    "build:npmjs": "ng-packagr -p ./src.statefuld/package.json",
    "publish:github": "env NPM_CONFIG_REGISTRY='https://npm.pkg.github.com/@timurxyz' npm publish ./src.statefuld/dist --access public",
    "publish:npmjs": "npm publish ./src.statefuld/dist --access public"
  }
}
